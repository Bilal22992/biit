{
  "family": "nodeapp-ecs",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::468907522523:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "nodeapp",
      "image": "468907522523.dkr.ecr.ap-southeast-1.amazonaws.com/nodeapp:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024"
}
